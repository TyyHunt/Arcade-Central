<h1 class="page-headers">ARCADES</h1>
<div class="container">
    <% @arcades.each do |arcade| %>
        <div class="card text-white bg-secondary mb-3 float-left inline" style="max-width: 21rem; width: 1000px; margin: 5px;">
                <button class="btn btn-primary" id="arcade-names" type="submit"><%= arcade.name %></button>
        </div>
    <% end %>
</div>
<br>
<div id="arcade-show">
</div>

